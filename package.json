{
  "name": "@noeticit/nitseditor-frontend",
  "version": "0.0.17",
  "description": "Front end scafolding for nitseditor",
  "main": "app.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/@noeticit/nitseditor-frontend/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/@noeticit/nitseditor-frontend/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/@noeticit/nitseditor-frontend/webpack.config.js",
    "icons": "vsvg -s assets/icons -t components/icons",
    "prebuild": "npm run icons",
    "build": "node build/build.js"
  },
  "keywords": [
    "nitseditor",
    "frontend"
  ],
  "author": {
    "name": "Nitish Kumar"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noeticit/nitseditor-frontend.git"
  },
  "homepage": "https://nitseditor.com",
  "dependencies": {
    "@aws-amplify/ui-vue": "^0.2.31",
    "@inertiajs/inertia": "^0.6.3",
    "@inertiajs/inertia-vue": "^0.4.3",
    "@popperjs/core": "^2.6.0",
    "@syncfusion/ej2-vue-dropdowns": "^18.3.53",
    "autoprefixer": "^9.8.6",
    "aws-amplify": "^3.3.12",
    "aws-amplify-vue": "^2.1.3",
    "axios": "^0.19",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0.3",
    "js-cookie": "^2.2.1",
    "laravel-mix": "^5.0.9",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "path-exists": "^4.0.0",
    "popper.js": "1.15.0",
    "postcss": "^7.0.35",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.2",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.30.0",
    "sass-loader": "^8.0.2",
    "sweetalert2": "^9.17.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.1",
    "tailwindcss-plugins": "^0.3.0",
    "tailwindcss-spinner": "^1.2.0",
    "v-click-outside": "^3.1.2",
    "vue": "^2.6.12",
    "vue-chat-scroll": "^1.4.0",
    "vue-js-modal": "^1.3.35",
    "vue-meta": "^2.4.0",
    "vue-route-generator": "^0.4.3",
    "vue-router": "^3.4.9",
    "vue-simple-spinner": "^1.2.10",
    "vue-svgicon": "^4.0.1-alpha.0",
    "vue-swatches": "^2.1.0",
    "vue-tailwind-modal": "0.0.7",
    "vue-template-compiler": "^2.6.12",
    "vue-virtual-scroller": "^1.0.10",
    "vue2-dropzone": "^3.6.0",
    "vue2-editor": "^2.10.2",
    "vuedraggable": "^2.24.3",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "^3.6.0",
    "vuex-persistedstate": "^3.1.0"
  }
}
