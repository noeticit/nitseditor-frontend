{
  "name": "nitseditor-frontend",
  "version": "0.0.2",
  "description": "Front end scafolding for nitseditor",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=webpack.config.js"
  },
  "keywords": [
    "nitseditor",
    "frontend"
  ],
  "author": "Nitish Kumar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noeticit/nitseditor.git"
  },
  "homepage": "https://nitseditor.com",
  "devDependencies": {
    "axios": "^0.19",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0",
    "laravel-mix": "^5.0.1",
    "lodash": "^4.17.13",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^1.2.0",
    "vue": "^2.6.11",
    "vue-auto-routing": "^0.4.1",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "laravel-mix-alias": "^1.0.2",
    "path-exists": "^4.0.0",
    "vuex": "^3.1.3"
  }
}
